  "scripts": {
    "dev": "cross-env NODE_ENV=development SERVER_ENV=development server.js",
    "build:dev": "cross-env-shell NODE_ENV=development 'next build && node bin/bundle'",
    "build:testing": "cross-env-shell NODE_ENV=testing 'next build && node bin/bundle'",
    "start:testing": "cross-env NODE_ENV=production SERVER_ENV=testing PORT=9000 node --max-http-header-size=10485760 server.js",
    "testing": "yarn run build:testing & yarn run start:testing",
    "build:staging": "cross-env-shell NODE_ENV=staging 'next build && node bin/bundle'",
    "start:staging": "cross-env NODE_ENV=production SERVER_ENV=staging PORT=8000 node --max-http-header-size=10485760 server.js",
    "staging": "yarn run build:staging & yarn run start:staging",
    "build:production": "cross-env-shell NODE_ENV=production 'next build && node bin/bundle'",
    "start:production": "cross-env NODE_ENV=production SERVER_ENV=production PORT=7000 node --max-http-header-size=10485760 server.js",
    "production": "yarn run build:production & yarn run start:production"
  },
